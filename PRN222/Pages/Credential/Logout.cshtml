@page
@model PRN222.Pages.Credential.LogoutModel
@{
    ViewData["Title"] = "Logout";
}

<h1>Logout</h1>

<form method="post">
    <p>Are you sure you want to logout?</p>
    <button type="submit" class="btn btn-danger">Logout</button>
    <a asp-page="/Index" class="btn btn-secondary">Cancel</a>
</form>
